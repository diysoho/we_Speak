/* pages/myindex/myindex.wxss */

page {
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.bg {
  width: 100%;
  height: 100%;
  position: relative;
  overflow: hidden;
}

.img {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0px;
  z-index: 0;
}

.avatar_box {
  position: absolute;
  top: 127rpx;
  text-align: center;
  height: 200rpx;
  overflow: hidden;
  width: 100%;
}

.avatar_box image {
  width: 180rpx;
  height: 180rpx;
  -webkit-border-radius: 999rpx;
  -moz-border-radius: 999rpx;
  border-radius: 999rpx;
  border: 10rpx solid white;
}

.location {
  position: relative;
  z-index: 1;
}

.text {
  font-family: PingFang SC;
  color: #FFE2B1;
  font-size: 40rpx;
  position: absolute;
  top: 387rpx;
  text-align: center;
  width:100%;
  left: 0;
}

@media screen and (min-width:350px) and (max-width:374px){
    .code {
      bottom: 330rpx !important;
  }
}

.code {
  height: 280rpx;
  width: 100%;
  z-index: 999;
  text-align:center;
  position: absolute;
  bottom: 380rpx;
  left: 0;
}

.picture {
  width: 280rpx;
  height: 280rpx;
  margin: 0 auto;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  border: 2rpx solid #fff;
}

.bottom {
  height: 220rpx;
  width: 80%;
  position: absolute;
  bottom: 70rpx;
  left: 10%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-around;
  -ms-flex-pack: distribute;
  justify-content: space-around;
  font-size: 38rpx;
  font-weight: 600;
}

.btn-area {
  width: 220rpx;
  height: 220rpx;
}

.jrong {
  height: 220rpx;
  color: #333;
  line-height: 220rpx;
  text-align: center;
  border: none;
  -webkit-border-radius: 999rpx;
  -moz-border-radius: 999rpx;
  border-radius: 999rpx;
  background: url("data:image/png;base64,") no-repeat 0 0/cover;
}

.btn-area button {
  font-size: 38rpx;
  color: #333;
  height: 100%;
  width: 100%;
  line-height: 220rpx;
  border: none;
  -webkit-border-radius: 999rpx;
  -moz-border-radius: 999rpx;
  border-radius: 999rpx;
  background: url("data:image/png;base64,") no-repeat 0 0/cover;
}

